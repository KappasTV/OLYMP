#include <iostream>
using namespace std;
int main() {
    string s;
    cin>>s;
    char c = '9';
    int index;
    for (int i = 0; i < s.size(); ++i) {
        if(c>s[i])
        {
            c = s[i];index = i;
        }
    }
    for (int i = 0; i < s.size(); ++i) {
        if(i!=index)
        {
            cout<<s[i];
        }
    }
    return 0;
}

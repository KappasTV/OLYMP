#include <iostream>
using namespace std;
int main() {
    string s;
    cin>>s;
    int signal=0;
    for (int i = 0; i < s.size()-1; ++i) {
        if(s[i]>=s[i+1])
        {
            cout<<"NO";
            signal=1;
            break;
        }
    }
    if(signal==0) cout<<"YES";
    return 0;
}
